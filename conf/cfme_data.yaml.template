basic_info:
    app_version: 5.2.0
management_systems:
    vsphere5:
        name: vsphere 5
        default_name: Virtual Center (10.0.0.2)
        credentials: vsphere5-credential_name_from_credentials.yaml
        hostname: vsphere5.example.com
        ipaddress: 10.0.0.1
        host_vnc_port:
            start: 5900
            end: 5980
        server_zone: default
        type: virtualcenter
        discovery_range:
            start: 10.0.0.1
            end: 10.0.0.2
        datacenters:
            - default
        clusters:
            - iscsi
        hosts:
            - name: myhost
              credentials: myhost
              type: esxi
              test_fleece: True
        datastores:
            - iscsi
        tags:
            Provisioning Scope: All
        policy_profiles:
            - example policy profile x
        test_vm_power_control:
            - vm_name
    rhevm31:
